<Window x:Class="WebSocketCliet.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebSocketCliet.Views"
        mc:Ignorable="d"
        Title="WebSocketClient" Height="350" Width="300">
    <DockPanel>
        <local:ConfigsView DockPanel.Dock="Top" DataContext="{Binding configsViewModel}" />
        <local:WSClientView DataContext="{Binding wsClientViewModel}" />
    </DockPanel>
</Window>
